{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
		"no-tabs": "off",
		"indent": ["error", "tab"],
		"linebreak-style": ["error", "windows"],
		"object-curly-spacing": ["error", "always"]
    }
}
